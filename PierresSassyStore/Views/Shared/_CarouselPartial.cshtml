@model List<string>
<div id="generalDisplayCarousel" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
        <img class="d-block w-100" src="~/img/hat.png" alt="First slide">
        <div class="carousel-caption d-none d-md-block">
            <h3><span>Pierre's Sassy Store</span></h3>
        </div>
    </div>
    @foreach(string img in Model)
    {
        @if(!string.IsNullOrEmpty(img))
        {
        <div class="carousel-item">
            <img class="d-block w-100" src="@img" alt="Image of a treat or flavor">
        </div>
        }
    }
    <a class="carousel-control-prev" href="#generalDisplayCarousel" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#generalDisplayCarousel" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>